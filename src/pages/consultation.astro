---
import Layout from "../layouts/Layout.astro";
const BASE = import.meta.env.BASE_URL;
---
<Layout title="Consultation | NexGen Binary" active="consultation">

<main class="container page">
  <div class="panel" id="book">
    <h1>Book a Consultation</h1>
    <form class="form">
      <label>Name *<input type="text" required /></label>
      <label>Email *<input type="email" required /></label>
      <label>Message *<textarea rows="5" required></textarea></label>
      <button type="submit" class="btn primary"><i class="fa-solid fa-calendar-check"></i> Submit</button>
    </form>
  </div>
</main>

</Layout>
