---
import "bootstrap/dist/css/bootstrap.min.css";
import '../styles/custom.css';
const { title = 'NexGen Binary', site } = Astro.props;
const formEndpoint = site?.contact?.formEndpoint || '';
const base = import.meta.env.BASE_URL || '/';
const asset = (name) => `${base}${name}`;
const inlineFavicon = `data:image/png;base64,`;
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <meta name="description" content="NexGen Binary provides dental-focused managed IT, security, cloud, and support services across Virginia." />
    <link rel="icon" type="image/png" href={inlineFavicon} />
    <link rel="shortcut icon" href={asset(`favicon.ico?v=41`)} />
    <link rel="icon" type="image/x-icon" href={asset(`favicon.ico?v=41`)} />
    <link rel="icon" type="image/png" sizes="32x32" href={asset(`favicon-32x32.png?v=41`)} />
    <link rel="icon" type="image/png" sizes="16x16" href={asset(`favicon-16x16.png?v=41`)} />
    <link rel="apple-touch-icon" href={asset(`apple-touch-icon.png?v=41`)} />
    <link rel="manifest" href={asset(`site.webmanifest?v=41`)} />
    <meta name="theme-color" content="#0b2f53" />
    <script is:inline>
      if ('scrollRestoration' in history) history.scrollRestoration = 'manual';
      const _nbScrollTop = () => {
        if (!window.location.hash) {
          try { window.scrollTo({ top: 0, left: 0, behavior: 'auto' }); } catch { window.scrollTo(0, 0); }
        }
      };
      window.addEventListener('DOMContentLoaded', _nbScrollTop);
      window.addEventListener('load', _nbScrollTop);
      window.addEventListener('pageshow', _nbScrollTop);
    </script>
  </head>
  <body>
    <slot />
    <script is:inline define:vars={{ formEndpoint }}>
      window.__NEXGEN_FORM_ENDPOINT__ = formEndpoint;
    </script>
  </body>
</html>
