---
const { title = "NexGen Binary", description = "Reliable IT for Dental Practices Across Virginia" } = Astro.props;
import "../styles/custom.css";
import "../styles/nav-stability-v17.css";
const inlineFavicon = "data:image/png;base64,";
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <meta name="description" content={description} />
    <meta name="theme-color" content="#0b2742" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg?v=32" />
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png?v=32" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png?v=32" />
    <link rel="icon" type="image/x-icon" href="/favicon.ico?v=32" />
    <link rel="shortcut icon" href="/favicon.ico?v=32" />
    <link rel="apple-touch-icon" href="/apple-touch-icon.png?v=32" />
    <link id="nb-favicon-inline" rel="icon" type="image/png" href={inlineFavicon} />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&family=Plus+Jakarta+Sans:wght@600;700;800&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  </head>
  <body>
    <slot />
    <script>
      (function() {
        try { history.scrollRestoration = "manual"; } catch(e) {}
        const applyFavicon = () => {
          const href = "data:image/png;base64,";
          let l = document.getElementById("nb-favicon-runtime");
          if (!l) {
            l = document.createElement("link");
            l.id = "nb-favicon-runtime";
            l.rel = "icon";
            l.type = "image/png";
            document.head.appendChild(l);
          }
          l.href = href;
        };
        const forceTop = () => {
          window.scrollTo({ top: 0, left: 0, behavior: "auto" });
          if (location.hash) {
            setTimeout(() => window.scrollTo(0,0), 0);
          }
        };
        applyFavicon();
        window.addEventListener("load", () => { applyFavicon(); forceTop(); });
        window.addEventListener("pageshow", () => { applyFavicon(); forceTop(); });
        document.addEventListener("visibilitychange", applyFavicon);
      })();
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
