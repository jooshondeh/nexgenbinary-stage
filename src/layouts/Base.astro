---
const { title = "NexGen Binary", description = "Reliable IT for Dental Practices Across Virginia" } = Astro.props;
import "../styles/custom.css";
import "../styles/nav-stability-v17.css";
const inlineFavicon = "data:image/png;base64,";
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <meta name="description" content={description} />
    <meta name="theme-color" content="#0b2742" />

    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png?v=31" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png?v=31" />
    <link rel="icon" type="image/x-icon" href="/favicon.ico?v=31" />
    <link rel="shortcut icon" href="/favicon.ico?v=31" />
    <link rel="apple-touch-icon" href="/apple-touch-icon.png?v=31" />
    <link id="nb-favicon-inline" rel="icon" type="image/png" href={inlineFavicon} />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&family=Plus+Jakarta+Sans:wght@600;700;800&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  </head>
  <body>
    <slot />
    <script>
      (function() {
        const href = "data:image/png;base64,";
        const apply = () => {
          let l = document.getElementById("nb-favicon-runtime");
          if (!l) { l = document.createElement("link"); l.id="nb-favicon-runtime"; l.rel="icon"; l.type="image/png"; document.head.appendChild(l); }
          l.href = href;
        };
        apply(); window.addEventListener("load", apply); document.addEventListener("visibilitychange", apply);
      })();
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
